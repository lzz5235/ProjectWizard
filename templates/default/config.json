{
  "project_name":"default",
  "project_description":"description",
  "files":
  [
    {
      "source":"project.pro",
      "target":"{{ config.project_name }}.pro"
    },
    {
      "source":"include/project_global.h",
      "target":"include/{{ config.project_name }}Def.h"
    },
    {
      "source":"include/project.h",
      "target":"include/{{ config.project_name }}.h"
    },
    {
      "source":"src/project.cpp",
      "target":"src/{{ config.project_name }}.cpp"
    },
    {
      "source":"scripts/clean.bat",
      "target":"scripts/clean.bat"
    },
    {
      "source":"scripts/cr-vcproj.bat",
      "target":"scripts/cr-vcproj.bat"
    },
    {
      "source":"scripts/VS02008-vsvars32.bat",
      "target":"scripts/VS02008-vsvars32.bat"
    },
    {
      "source":"scripts/make.bat",
      "target":"scripts/make.bat"
    },
    {
      "source":"scripts/clean.sh",
      "target":"scripts/clean.sh"
    },
    {
      "source":"scripts/make.sh",
      "target":"scripts/make.sh"
    }
    {% if config.component_type == "window" %}
    ,{
      "source":"forms/project.ui",
      "target":"forms/{{ config.project_name }}.ui"
    },
    {
      "source":"include/ui_project.h",
      "target":"include/ui_{{ config.project_name }}.h"
    }
    {% endif %}
    {% if config.resourcesFile == "True" %}
    ,{
      "source":"project.qrc",
      "target":"{{ config.project_name }}.qrc"
    }
    {% endif %}
    {% if config.translateFile == "True" %}
    ,{
      "source":"translations/project.ts",
      "target":"translations/{{ config.project_name }}.ts"
    }
    {% endif %}
  ],
  "moudels":
  [
    {
      "name":"公共工具库",
      "description":"提供通用的工具",
      "buildin":true,
      "libs":
              [
                {
                   "prefix":"",
                    "name":"AllOneFoundation"
                }
              ]
    },
    {
      "name":"事件通知",
      "description":"事件通知的介绍",
      "buildin":true,
      "libs":
              [
                {
                   "prefix":"",
                    "name":"EventNotification"
                }
              ]
    },
    {
      "name":"关系数据库",
      "description":"关系数据库的工具",
      "buildin":false,
      "libs":
              [
                {
                   "prefix":"",
                    "name":"DBManagerComp"
                },
                {
                   "prefix":"",
                    "name":"DBCommon"
                }
              ]
    },
    {
      "name":"UDP通信",
      "description":"UPD通信组件",
      "buildin":false,
      "libs":
              [
                {
                   "prefix":"",
                    "name":"MsgConverter"
                },
                {
                   "prefix":"",
                    "name":"UDPCommunication"
                }
              ]
    },
    {
      "name":"DDS通信",
      "description":"DDS通信的介绍",
      "buildin":false,
      "libs":
              [
                {
                   "prefix":"",
                    "name":"DDSMsgHandle"
                }
              ]
    },
    {
      "name":"实时数据库",
      "description":"实时数据库的介绍",
      "buildin":false,
      "libs":
              [
                {
                   "prefix":"",
                    "name":"RTDBDriver"
                }
              ]
    }
  ],
  "interfaces":
  [
	{
      "name":"changeShowMode",
      "description":"改变显示模式",
      "flag_win":2,
      "flag_service":0
    },
    {
      "name":"curScreenDescribe",
      "description":"获取当前屏幕描述",
      "flag_win":2,
      "flag_service":0
    },
    {
      "name":"workModelChange",
      "description":"工作模式切换",
      "flag_win":2,
      "flag_service":3
    },
    {
      "name":"initialize",
      "description":"初始化对象",
      "flag_win":0,
      "flag_service":0
    },
    {
      "name":"initData",
      "description":"初始化数据",
      "flag_win":2,
      "flag_service":3
    },
    {
      "name":"onListenerCompStatusUpdate",
      "description":"被监听的功能组件状态改变",
      "flag_win":1,
      "flag_service":3
    },
    {
      "name":"showComp",
      "description":"显示功能组件",
      "flag_win":1,
      "flag_service":3
    },
    {
      "name":"hideComp",
      "description":"隐藏功能组件",
      "flag_win":1,
      "flag_service":3
    },
    {
      "name":"suspendComp",
      "description":"挂起功能组件",
      "flag_win":1,
      "flag_service":3
    },
    {
      "name":"resumeComp",
      "description":"恢复功能组件",
      "flag_win":1,
      "flag_service":3
    },
    {
      "name":"terminateComp",
      "description":"卸载功能组件",
      "flag_win":1,
      "flag_service":3
    },
    {
      "name":"run",
      "description":"对象执行功能",
      "flag_win":1,
      "flag_service":3
    },
    {
      "name":"saveCompMemento",
      "description":"保存组件备忘录数据",
      "flag_win":1,
      "flag_service":3
    },
    {
      "name":"resumeCompMemento",
      "description":"从备忘录中恢复数据",
      "flag_win":1,
      "flag_service":3
    }
  ]
}